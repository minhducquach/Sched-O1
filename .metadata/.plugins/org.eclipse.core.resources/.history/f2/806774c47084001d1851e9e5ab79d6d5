#include "sched.h"

/******** STRUCT & LIST IMPLEMENTATION ********/

typedef struct {
	void 		(*pTask)(void);
	uint32_t 	delay;
	uint32_t 	period;
	uint8_t 	runFlag;
	uint32_t 	taskID;
} Task;

